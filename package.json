  "description": "Strip out any mongoose info from a document",
  "main": "lib/mongoose-cleaner.js",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "lint": "eslint .",
    "commit": "commit-wizard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "rm -rf lib && babel src --out-dir lib",
    "prepublish": "npm run build",
    "postpublish": "greenkeeper-postpublish"
  },
  "author": "Springworks",
  "devDependencies": {
    "@springworks/test-harness": "1.3.5",
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-register": "6.9.0",
    "eslint": "2.10.2",
    "eslint-config-springworks": "7.0.3",
    "eslint-plugin-import": "1.8.0",
    "eslint-plugin-mocha": "2.2.0",
    "eslint-plugin-should-promised": "1.0.8",
    "eslint-plugin-springworks": "2.0.1",
    "greenkeeper-postpublish": "1.0.0",
    "mocha": "2.4.5",
    "mongoose": "4.4.17",
    "pre-git": "3.8.4",
    "semantic-release": "4.3.5"
  },
  "dependencies": {
    "lodash.mapvalues": "4.4.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.7"
