{
  "name": "@springworks/mongoose-cleaner",
  "description": "Strip out any mongoose info from a document",
  "main": "lib/mongoose-cleaner.js",
  "scripts": {
    "test": "NODE_ENV=test _mocha -- test",
    "lint": "eslint --cache .",
    "commit": "commit-wizard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "rm -rf lib && babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "author": "Springworks",
  "repository": {
    "type": "git",
    "url": "git@github.com:Springworks/mongoose-cleaner.git"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.3.1",
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-plugin-transform-strict-mode": "6.6.5",
    "babel-preset-es2015-node4": "2.1.0",
    "eslint": "2.7.0",
    "eslint-config-springworks": "7.0.1",
    "eslint-plugin-import": "1.4.0",
    "eslint-plugin-mocha": "2.1.0",
    "eslint-plugin-should-promised": "1.0.8",
    "eslint-plugin-springworks": "1.1.2",
    "mocha": "2.4.5",
    "mongoose": "4.4.13",
    "pre-git": "3.8.3",
    "semantic-release": "4.3.5"
  },
  "dependencies": {
    "lodash.mapvalues": "4.3.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.7"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  }
}
